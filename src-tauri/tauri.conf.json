{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "DeluxeMenusEditor",
  "version": "0.0.11",
  "identifier": "com.tauri.deluX",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8080",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "杜蕾斯菜单编辑器0.11 by-yellow_Gay",
        "width": 1200,
        "height": 850,
        "resizable": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
